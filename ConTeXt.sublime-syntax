      captures: {1: punctuation.section.braces.begin.context, 2: support.function.general.context meta.other.control.word.context, 3: punctuation.definition.backslash.context}
      push: ConTeXt.group.font.boldface.main/
  ConTeXt.group.toggle.italic:
