        - include: MetaFun.main
  MetaFun.block.staticMPfigure.main/:
    - match: ''
      set:
        - meta_content_scope: source.metapost.metafun.embedded.context
        - match: '(?=\\stopstaticMPfigure\b)'
          pop: true
        - include: MetaFun.main
  MetaFun.block.MPdefinitions.main/:
    - match: ''
      set:
        - meta_content_scope: source.metapost.metafun.embedded.context
        - match: '(?=\\stopMPdefinitions\b)'
          pop: true
        - include: MetaFun.main
  MetaFun.block.MPinclusions.main/:
    - match: ''
      set:
        - meta_content_scope: source.metapost.metafun.embedded.context
        - match: '(?=\\stopMPinclusions\b)'
          pop: true
        - include: MetaFun.main
  MetaFun.block.MPextensions.main/:
    - match: ''
      set:
        - meta_content_scope: source.metapost.metafun.embedded.context
        - match: '(?=\\stopMPextensions\b)'
          pop: true
        - include: MetaFun.main

  MetaFun.main:
    - include: templates
    - include: MetaFun.TeX
    - include: scope:source.metapost.metafun
    - include: MetaFun.control.sequence

  MetaFun.control.sequence:
    - include: Lua
    - include: control.symbol
    - include: control.word.generic

  MetaFun.TeX:
    - match: '\b(b|verbatim)tex\b'
      scope: entity.name.other.tex.metapost
      push:
        - meta_content_scope: text.tex.context.embedded.metafun
        - match: '(?=\betex\b)'
          pop: true
        - include: main

  templates:
    - match: '(<\?)(lua)\b'
      captures: {1: punctuation.definition.tag.begin.context, 2: entity.name.tag.lua.context}
      push:
        - meta_include_prototype: false
      # - meta_scope: meta.preprocessor.lua.context meta.tag.lua.context
        - meta_content_scope: source.lua.embedded.context
        - match: '\?>'
          scope: punctuation.definition.tag.end.context
          pop: true
        - include: templates.main

  templates.main:
    - include: templates.Lua.functions
    - include: scope:source.lua
  templates.Lua.functions:
    - match: '\b(inject)\b'
      scope: support.function.lua

  ConTeXt:
    - include: ConTeXt.protect
    - include: ConTeXt.asciimode
    - include: ConTeXt.comments
    - include: ConTeXt.defs
    - include: ConTeXt.setups
    - include: ConTeXt.modules
    - include: ConTeXt.headings
    - include: ConTeXt.verbatim
    - include: ConTeXt.quotes
    - include: ConTeXt.font
    - include: ConTeXt.tables

  ConTeXt.protect:
    - match: '(\\)unprotect\b'
      captures: {0: support.function.general.context meta.other.control.word.context, 1: punctuation.definition.backslash.context}
      push:
        - meta_content_scope: meta.other.unprotect.context
        - match: '(?=\\protect\b)'
          pop: true
        - include: ConTeXt.protect.main

  ConTeXt.protect.main:
    - include: math
    - include: Lua
    - include: MetaFun
    - include: templates
    - include: ConTeXt
    - include: TeX
    - include: ConTeXt.protect.control
    - include: ConTeXt.protect.group

  ConTeXt.protect.control:
    - match: '(\\)([[:alpha:]_@!?]*[_@!?]+[[:alpha:]_@!?]*)'
      captures: {0: support.function.general.context meta.other.control.word.context, 1: punctuation.definition.backslash.context}
      push: ConTeXt.protect.lists-or-groups*/
    - include: control.word.without-generic
    - match: '(\\)[[:alpha:]]+'
      captures: {0: support.function.general.context meta.other.control.word.context, 1: punctuation.definition.backslash.context}
      push: ConTeXt.protect.lists-or-groups*/
    - include: control.symbol
    - include: control.active-character

  ConTeXt.protect.group:
    - match: '\{'
      scope: punctuation.section.braces.begin.context
      push: ConTeXt.protect.group.main/
  ConTeXt.protect.group.main/:
    - meta_scope: meta.braces.context
    - include: generic.pop-at-group-end/
    - include: ConTeXt.protect.main

  ConTeXt.protect.list:
    - match: '\['
      scope: punctuation.section.brackets.begin.context
      push: ConTeXt.protect.list.main/
  ConTeXt.protect.list.main/:
    - meta_scope: meta.brackets.context
    - include: generic.pop-at-list-end/
    - include: ConTeXt.protect.list.assignments
    - include: generic.dimension
    - include: ConTeXt.protect.main

  ConTeXt.protect.list.assignments:
    - match: '({{key}}+)(=)'
      captures: {1: variable.parameter.context, 2: keyword.operator.assignment.context}
      push:
        - meta_content_scope: meta.other.value.context
        - include: generic.list.pop-at-end-of-assignment/
        - include: generic.dimension
        - include: ConTeXt.protect.main
    - include: generic.assignment.common

  ConTeXt.protect.list-or-group:
    - include: ConTeXt.protect.list
    - include: ConTeXt.protect.group
  ConTeXt.protect.lists-or-groups*/:
    - include: ConTeXt.protect.list-or-group
    - include: generic.pop-if-no-nearby-list-or-group/

  ConTeXt.asciimode:
    - match: '(\\)start(asciimode)\b'
      captures: {0: support.function.general.context meta.other.control.word.context, 1: punctuation.definition.backslash.context}
      push:
        - meta_include_prototype: false
        - meta_content_scope: meta.other.asciimode.context
        - include: generic.pop-if-stop-match-two/
        - include: ConTeXt.asciimode.main
    - match: '(\\)asciimode\b'
      captures: {0: support.function.general.context meta.other.control.word.context, 1: punctuation.definition.backslash.context}
      push:
        - meta_include_prototype: false
        - meta_content_scope: meta.other.asciimode.context
        - include: ConTeXt.asciimode.main

  ConTeXt.asciimode.comment:
    - match: '(%%).*$\n?'
      captures: {0: comment.line.double-percentage.context, 1: punctuation.definition.comment.context}

  ConTeXt.asciimode.main:
    - include: ConTeXt.asciimode.comment
    - include: math.display.control
    - include: math.inline.control
    - include: Lua
    - include: MetaFun
    - include: templates
    - include: ConTeXt
    - include: ConTeXt.asciimode.control
    - include: ConTeXt.asciimode.group

  ConTeXt.asciimode.group:
    - match: '\{'
      scope: punctuation.section.braces.begin.context
      push: ConTeXt.asciimode.group.main/
  ConTeXt.asciimode.group.main/:
    - meta_include_prototype: false
    - meta_scope: meta.braces.context
    - include: generic.pop-at-group-end/
    - include: ConTeXt.asciimode.main

  ConTeXt.asciimode.list:
    - match: '\['
      scope: punctuation.section.brackets.begin.context
      push: ConTeXt.asciimode.list.main/
  ConTeXt.asciimode.list.main/:
    - meta_include_prototype: false
    - meta_scope: meta.brackets.context
    - include: generic.pop-at-list-end/
    - include: ConTeXt.asciimode.list.assignments
    - include: generic.dimension
    - include: ConTeXt.asciimode.main

  ConTeXt.asciimode.list.assignments:
    - match: '({{key}}+)(=)'
      captures: {1: variable.parameter.context, 2: keyword.operator.assignment.context}
      push:
        - meta_include_prototype: false
        - meta_content_scope: meta.other.value.context
        - include: generic.list.pop-at-end-of-assignment/
        - include: generic.dimension
        - include: ConTeXt.asciimode.main
    - include: generic.assignment.common

  ConTeXt.asciimode.list-or-group:
    - include: ConTeXt.asciimode.list
    - include: ConTeXt.asciimode.group
  ConTeXt.asciimode.lists-or-groups*/:
    - meta_include_prototype: false
    - include: ConTeXt.asciimode.list-or-group
    - include: generic.pop-if-no-nearby-list-or-group/

  ConTeXt.asciimode.control:
    - include: control.word.without-generic
